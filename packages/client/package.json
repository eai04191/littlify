        "@types/spotify-web-playback-sdk": "^0.1.7",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "stylelint": "^13.1.0",
        "stylelint-config-recommended": "^3.0.0",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "autoprefixer": "^9.7.4",
        "axios": "^0.19.2",
        "classnames": "^2.2.6",
        "history": "^4.10.1",
